                    "Get-ChildItem \"$dir\\extracted\\Chez Scheme *\\*\" | ForEach-Object { Move-Item $_ -Destination \"$dir\" -Force }",
                    "@('extracted') | ForEach-Object { Remove-Item \"$dir\\$_\" -Recurse }"
                ]
            }
        }
    },
    "bin": [
