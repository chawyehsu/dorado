            "hash": "f92fb9f03c0a033642a5f1e87e0517872d4d3f27ced0a80edcb9c4f81636419d"
        }
    },
    "bin": "pwsh.exe",
    "shortcuts": [
        [
            "pwsh.exe",
            "PowerShell Preview"
        ]
    ],
    "installer": {
        "script":[
            "$Documents = [Environment]::GetFolderPath('MyDocuments')",
            "cd $Documents",
            "if (!(Test-Path \"PowerShell\")) {",
            "   if (is_directory \"WindowsPowerShell\") {",
            "       New-Item \"PowerShell\" -ItemType Junction -Target \"WindowsPowerShell\" | Out-Null",
            "   }",
            "}",
            "cd $HOME"
        ]
    },
    "checkver": {
        "url": "https://www.github.com/PowerShell/PowerShell/releases",
        "regex": "v([\\d.]+-[\\w.]+)</"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/PowerShell/PowerShell/releases/download/v$version/PowerShell-$version-win-x64.zip"
            },
            "32bit": {
                "url": "https://github.com/PowerShell/PowerShell/releases/download/v$version/PowerShell-$version-win-x86.zip"
            }
        },
        "hash": {
            "url": "https://github.com/PowerShell/PowerShell/releases/tag/v$version/",
            "regex": "$basename\\s*<ul>\\s*<li>($sha256)"
        }
    }
}
