    "architecture": {
        "64bit": {
            "url": "https://download.mozilla.org/?product=firefox-69.0.1-SSL&os=win64&lang=zh-CN#/firefox.7z",
            "hash": "b851146226fddee938d89da8e42dcfe14aaa166b5acb9e89db85455f421eeeac"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-69.0.1-SSL&os=win&lang=zh-CN#/firefox.7z",
