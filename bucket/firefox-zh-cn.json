    "architecture": {
        "64bit": {
            "url": "https://download.mozilla.org/?product=firefox-73.0.1-SSL&os=win64&lang=zh-CN#/firefox.7z",
            "hash": "79837a1e9c712ed48dc2c2a0c369468cd176b8eccc5240ae734c12457026237d"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-73.0.1-SSL&os=win&lang=zh-CN#/firefox.7z",
            "hash": "e8ef96e932e623e66ce3c65e61e0251ef4d688ee51acb73fdb08f62d119f7051"
        }
    },
    "extract_dir": "core",
    "bin": "firefox.exe",
    "shortcuts": [
        [
            "firefox.exe",
            "Mozilla Firefox"
        ]
    ],
    "checkver": {
        "url": "https://www.mozilla.org/zh-CN/firefox/all/",
        "re": "data-latest-firefox=\"([\\d.]+)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win64&lang=zh-CN#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win64/zh-CN/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            },
            "32bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win&lang=zh-CN#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win32/zh-CN/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            }
        }
    }
}
