    "architecture": {
        "64bit": {
            "url": "https://download.mozilla.org/?product=firefox-76.0-SSL&os=win64&lang=zh-CN#/firefox.7z",
            "hash": "20bace55ff38a22b3765ffd0aa831f1b0030e69562b99e28236a5ac4acffc550"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-76.0-SSL&os=win&lang=zh-CN#/firefox.7z",
