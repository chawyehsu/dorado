{
    "homepage": "https://yarnpkg.com/",
    "description": "Fast, reliable, and secure dependency management.",
    "license": "BSD-2-Clause",
    "version": "1.22.1",
    "suggest": {
        "Node.js": [
            "nodejs",
            "nodejs-lts"
        ],
        "NVM": "dorado/nvm-windows"
    },
    "url": "https://yarnpkg.com/downloads/1.22.1/yarn-1.22.1.msi",
    "hash": "96ed3a31378cdaedddc843bc3f730ccef620733c3e7708da00a587ad8b3c33d8",
    "installer": {
        "script": "add_first_in_path \"$env:LOCALAPPDATA\\Yarn\\bin\" $global"
    },
    "uninstaller": {
        "script": "remove_from_path \"$env:LOCALAPPDATA\\Yarn\\bin\" $global"
    },
    "env_add_path": "Yarn\\bin",
    "checkver": {
        "url": "https://yarnpkg.com/latest-version",
        "re": "([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://yarnpkg.com/downloads/$version/yarn-$version.msi"
    }
}
