        "32bit": {
            "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/104.0b3/win32/zh-CN/Firefox%20Setup%20104.0b3.exe#/firefox-beta.7z",
            "hash": "sha512:8d12ff691e375b73541de249e86a2559290b7558fe9041aa353bde7a686e47db3941d8047a9f46886cd6201c10932393d11c2e8173d2ef5b0883b0c90ac48b83"
