        "32bit": {
            "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/120.0b7/win32/zh-CN/Firefox%20Setup%20120.0b7.exe#/firefox-beta.7z",
            "hash": "sha512:bf88e62e80841a041e2386139ea10fc2f982bf262250306a060117f7dbdc71a7fee55170d459bd4362955dd81d38a8406dbdceba7bd19b6db35dfe2171ecb894"
