        "32bit": {
            "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/119.0b5/win32/zh-CN/Firefox%20Setup%20119.0b5.exe#/firefox-beta.7z",
            "hash": "sha512:31106270b33611f099e393c194eba7bb28ee75900f703381dc124d0994c7b7738cb7464ce8886057d16e936624055c8a482b326e335ad9556842d42e1f98d2a6"
