        "32bit": {
            "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/130.0b8/win32/zh-CN/Firefox%20Setup%20130.0b8.exe#/firefox-beta.7z",
            "hash": "sha512:3d605ab462578ef0f505413317c22f6c86996e9cd222cd8885ef1bd9c5ff7094b09a216862247c76ccdaf3115fd453d2597daf4970b371059fc535ca9e29de34"
