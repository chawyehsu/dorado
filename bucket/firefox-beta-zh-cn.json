        "32bit": {
            "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/120.0b2/win32/zh-CN/Firefox%20Setup%20120.0b2.exe#/firefox-beta.7z",
            "hash": "sha512:0d79ad2c0ada0b8e16dbb0bf23801b8112e219bc4d1c91507d61ea1d0af5efe65122cf7372f87a7944fa4b6360fbba280ebe354f8f250039586f9e00105a78ad"
