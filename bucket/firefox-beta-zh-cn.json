        "32bit": {
            "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/110.0b4/win32/zh-CN/Firefox%20Setup%20110.0b4.exe#/firefox-beta.7z",
            "hash": "sha512:b77b26e74bdb8713ad6231ad95ea975095048bf3167f22767b4ae6802f51b3fed011963f4892edab11968cb4c9af9e086f93c09b013f4b4520531404893aa412"
