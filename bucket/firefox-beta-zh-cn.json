        "32bit": {
            "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/110.0b5/win32/zh-CN/Firefox%20Setup%20110.0b5.exe#/firefox-beta.7z",
            "hash": "sha512:ca9e859cfc28647f2acc1fc91bbedf9f00cc4d5cf367ff2b7c91af15263cada22d07a651f0e0a9fcfcf7f0e87e407baced79823e6f652e6f874d542caf589bea"
