        "32bit": {
            "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/123.0b5/win32/zh-CN/Firefox%20Setup%20123.0b5.exe#/firefox-beta.7z",
            "hash": "sha512:62596cea1fe009bac11002ddb3c08c00b8cdd33f2ce941960772ed41b58d39fc6d1ac63ba6c8353679039396b2bebb034ffc9018960d2751103eb5b809165a79"
