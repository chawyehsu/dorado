        "32bit": {
            "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/133.0b4/win32/zh-CN/Firefox%20Setup%20133.0b4.exe#/firefox-beta.7z",
            "hash": "sha512:9b839cf81227ede5f7aed1389f673952df8323563f6c0607644fc6f636adb3755bf0794cf273025d2f7862ffe84df956e4e30da7667b1b105ac6327d17e48032"
