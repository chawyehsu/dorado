    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://registry.yarnpkg.com/esbuild-windows-64/-/esbuild-windows-64-0.14.31.tgz",
            "hash": "sha1:5d4b0ea686c9b60036303b3415c472f2761bdafc"
        },
        "32bit": {
            "url": "https://registry.yarnpkg.com/esbuild-windows-32/-/esbuild-windows-32-0.14.31.tgz",
