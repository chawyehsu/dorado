    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://registry.yarnpkg.com/esbuild-windows-64/-/esbuild-windows-64-0.8.55.tgz",
            "hash": "sha1:5783d55f4530b97fb542c98d364c3439d312b622"
        },
        "32bit": {
